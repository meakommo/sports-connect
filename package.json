    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-async-script": "^0.9.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.11.0",
    "webpack-livereload-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-runtime": "^6.26.0",
    "nodemon": "^1.15.0"
